
@{
    ViewBag.Title = "MyCloset";
    //This is the Foundation MVC template
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<head>
    <link href="~/Content/MyCloset.css" rel="stylesheet" type="text/css" />
    <link href="~/Content/foundation/foundation.css" rel="stylesheet" type="text/css" />
</head>
<body>

<div class="row">
    <h2>@ViewBag.Message</h2>
</div>


    <div class="row medium-up-7">
        @foreach (var std in ViewBag.Clothes)
        {
            <div class="column large-4 columns">
   
                <img src="@std" alt="">
            </div>
            
        }
    </div>


    <div class="row">
        <a href="#" class="button">Add Clothes</a>
        <a href="#" class="button">Share</a>
    </div>

    


</body>
